  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@cashu/cashu-ts": "^0.9.0",
    "@gandlaf21/bolt11-decode": "^3.0.6",
